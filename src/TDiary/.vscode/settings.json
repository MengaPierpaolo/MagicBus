// Workspace settings
{
    // Hide the following when working in vscode
    "files.exclude": {
        "bin/": true,
        "obj/": true,
        
        "Properties" : true, // This or the next two
        //"Properties/launchSettings.json": true, // VS launch file
        //"Properties/PublishProfiles/" : true, // VS Publish Profiles
        
        "*.user": true, // VS user settings files
        "*.xproj": true, // VS Project Files
        
        "project.lock.json": true // Never manually-edited lock file
    }
}